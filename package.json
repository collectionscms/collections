{
  "name": "superfastcms",
  "version": "0.10.1",
  "description": "Free and Open-source Headless CMS and Application Framework built with TypeScript, Node.js, React.",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.19.0"
